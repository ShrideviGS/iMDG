<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:table="sap.ui.table">
	<Dialog showHeader="false">
		<Toolbar height="2rem" class="toolBarClass">
			<Label text="{i18n>EXTENDED_VALUE_SELECTOR}:"></Label>
		</Toolbar>
		<table:Table rows="">
			<table:columns>
				<table:Column>
					<Label text="{i18n>VALUE}"/>
					<table:template>
						<Text text="" wrapping="false"/>
					</table:template>
				</table:Column>
				<table:Column width="10rem">
					<Label text="{i18n>DESCRIPTION}"/>
					<table:template>
						<Text text="" wrapping="false"/>
					</table:template>
				</table:Column>
			</table:columns>
		</table:Table>
	</Dialog>
</core:FragmentDefinition>