							<core:CustomData key="target" value="INPUT"/>
						</customData>
					</Input>
				</VBox>
			</l:content>
		</l:Grid>
		<Toolbar height="2rem">
			<ToolbarSpacer></ToolbarSpacer>
			<Button text="{i18n>ADD}" icon="sap-icon://add" type="Accept" press="onAddUOMPerEANTableRow"></Button>
